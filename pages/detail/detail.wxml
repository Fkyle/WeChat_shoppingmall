<!--pages/detail/detail.wxml-->
<view id="detail">
  <f-nav-bar class='nav-bar' titles="{{titles}}"/><view class="block"></view>
  <f-detail-swiper class="detail-swiper" swiperList="{{topImages}}"/>
  <f-back-top wx:if="{{isShowBackTop}}"/>
  <detail-base-info goods="{{goods}}" />
  <detail-shop-info shop="{{shop}}" />
  <detail-goods-info detailInfo="{{detailInfo}}"/>
  <detail-params-info paramsInfo="{{paramsInfo}}"/>
  <detail-comment-info commentInfo="{{commentInfo}}"/>
  <detail-recommend-info recommendInfo="{{recommendInfo}}"/>
</view>